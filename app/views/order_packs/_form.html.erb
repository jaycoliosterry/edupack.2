<%= simple_form_for(@order_pack) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :weight %>
    <%= f.input :width %>
    <%= f.input :length %>
    <%= f.input :depth %>
    <%= f.input :customs %>
    <%= f.input :customs_detail %>
    <%= f.input :description %>
    <%= f.input :quantity %>
    <%= f.input :customs_value %>
    <%= f.input :cost %>
    <%= f.association :order %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
