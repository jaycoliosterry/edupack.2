<%- model_class = OrderPack -%>
<div class="page-header">
  <h1><%=t '.title', :default => OrderPack %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:weight) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.weight %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:width) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.width %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:length) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.length %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:depth) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.depth %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customs) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.customs %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customs_detail) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.customs_detail %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:quantity) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.quantity %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customs_value) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.customs_value %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cost) %></dt>
      <dd><pre class="prettyprint"><%= @order_pack.cost %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              order_packs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_order_pack_path(@order_pack), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              order_pack_path(@order_pack),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

