<h1>Listing stock_attachments</h1>

<table>
  <thead>
    <tr>
      <th>Stock</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_attachments.each do |stock_attachment| %>
      <tr>
        <td><%= stock_attachment.stock %></td>
        <td><%= stock_attachment.url %></td>
        <td><%= link_to 'Show', stock_attachment %></td>
        <td><%= link_to 'Edit', edit_stock_attachment_path(stock_attachment) %></td>
        <td><%= link_to 'Destroy', stock_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock attachment', new_stock_attachment_path %>
