<h1>Listing order_pack_items</h1>

<table>
  <thead>
    <tr>
      <th>Quantity</th>
      <th>Stock</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_pack_items.each do |order_pack_item| %>
      <tr>
        <td><%= order_pack_item.quantity %></td>
        <td><%= order_pack_item.stock %></td>
        <td><%= order_pack_item.order %></td>
        <td><%= link_to 'Show', order_pack_item %></td>
        <td><%= link_to 'Edit', edit_order_pack_item_path(order_pack_item) %></td>
        <td><%= link_to 'Destroy', order_pack_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order pack item', new_order_pack_item_path %>
