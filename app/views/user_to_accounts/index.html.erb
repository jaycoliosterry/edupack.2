<h1>Listing user_to_accounts</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_to_accounts.each do |user_to_account| %>
      <tr>
        <td><%= user_to_account.account %></td>
        <td><%= user_to_account.user %></td>
        <td><%= link_to 'Show', user_to_account %></td>
        <td><%= link_to 'Edit', edit_user_to_account_path(user_to_account) %></td>
        <td><%= link_to 'Destroy', user_to_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User to account', new_user_to_account_path %>
