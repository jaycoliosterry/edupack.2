<%= simple_form_for(@order_pack_package) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :origin %>
    <%= f.input :destination %>
    <%= f.input :status %>
    <%= f.input :history %>
    <%= f.input :waybill %>
    <%= f.input :total_pieces %>
    <%= f.input :piece %>
    <%= f.association :order %>
    <%= f.association :order_pack %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
