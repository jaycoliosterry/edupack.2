<%- model_class = EconomyPrice -%>
<div class="page-header">
  <h1><%=t '.title', :default => EconomyPrice %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:weight) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.weight %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone1) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone2) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone3) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone3 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone4) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone4 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone5) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone5 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone6) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone6 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone7) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone7 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone8) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone8 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone9) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone9 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone10) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone10 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone11) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone11 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone12) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone12 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zone13) %></dt>
      <dd><pre class="prettyprint"><%= @economy_price.zone13 %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              economy_prices_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_economy_price_path(@economy_price), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              economy_price_path(@economy_price),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

