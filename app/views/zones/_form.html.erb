<%= simple_form_for(@zone) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :country %>
    <%= f.input :zone %>
    <%= f.input :expresstime %>
    <%= f.input :economytime %>
    <%= f.input :iva %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
